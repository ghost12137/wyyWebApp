\r\n  },\r\n  data() {\r\n    return {\r\n      scrollByX: true, //判断是否横向滚动\r\n      currentIndex: -1, //点击的index\r\n    };\r\n  },\r\n  props: {\r\n    circleBtns: {\r\n      type: Array,\r\n      default() {\r\n        return [];\r\n      },\r\n    },\r\n  },\r\n  methods: {\r\n    circleClick(index) {\r\n      this.$emit('circleClick', index);\r\n      if (!this.$store.state.isLogin)\r\n        Toast.fail('请先登录');\r\n      else {\r\n        switch (index) {\r\n          case 0:\r\n            this.$router.replace('/dailyrecommend');\r\n            break;\r\n          case 1:\r\n            break;\r\n          case 2:\r\n            this.$router.replace('/songsquare');\r\n            break;\r\n          default:\r\n            break;\r\n        }\r\n      }\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.circle-scroll {\r\n  /* width: 100% !important; */\r\n  display: flex;\r\n  margin-left: 18px;\r\n  margin-top: 10px;\r\n  margin-bottom: 10px;\r\n}\r\n.content {\r\n  display: flex;\r\n  overflow: hidden;\r\n}\r\n.scroll-item {\r\n  margin-right: 10px;\r\n  font-size: 10px;\r\n  text-align: center;\r\n}\r\n.scroll-item img {\r\n  height: 55px;\r\n  width: 55px;\r\n  background-color: #fff1f1;\r\n  border-radius: 50%;\r\n  margin-bottom: 2px;\r\n}\r\n</style>"]}]}