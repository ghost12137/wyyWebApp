CA,aAAA,MAAA,GAAA,IAAA;AACA,OAFA,MAEA;AACA,aAAA,MAAA,GAAA,KAAA;AACA;AACA,KAZA;;AAaA;AACA;AACA;AACA;AACA,IAAA,eAjBA,2BAiBA,EAjBA,EAiBA;AAAA;;AACA,aAAA,OAAA,CAAA;AACA,QAAA,QAAA,EAAA,CADA;AAEA,QAAA,WAAA,EAAA,IAFA;AAGA,QAAA,OAAA,EAAA;AAHA,OAAA;;AAKA,MAAA,gBAAA,CAAA,EAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,yCAAA,EAAA,GAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,QAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,GAAA,CAAA,QAAA,CAAA,WAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,GAAA,CAAA,QAAA,CAAA,WAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,QAAA,CAAA,SAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,GAAA,CAAA,QAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,eAAA,GAAA,GAAA,CAAA,QAAA,CAAA,eAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,QAAA,CAAA,UAAA;AACA,OAXA,EAYA,KAZA,CAYA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,2CAAA,EAAA,GAAA;AACA,OAdA,EAeA,OAfA,CAeA,YAAA;AACA,eAAA,KAAA;AACA,OAjBA;AAkBA;AAzCA,GAtBA;AAiEA,EAAA,OAjEA,qBAiEA;AACA;AACA,SAAA,EAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,EAAA;AACA,SAAA,eAAA,CAAA,KAAA,EAAA;AACA;AArEA,CAAA","sourcesContent":["<template>\r\n  <div class=\"song-order-msg\">\r\n    <div class=\"play-all\" :class=\"isFlow==true?'active':'none'\">\r\n        <img src=\"~assets/img/dailyrecommend/playall/play.svg\" alt=\"\">\r\n        <span>播放全部</span>\r\n    </div>\r\n    <scroll class=\"songorder-scroll\" \r\n      @scroll=\"contentScroll\" \r\n      :isListen=\"2\">\r\n      <!-- 背景 -->\r\n      <background \r\n        :song-order-img=\"songOrderImg\" \r\n        :name=\"name\" \r\n        :description=\"description\"/>\r\n      <!-- 收藏等按钮 -->\r\n      <collect-etc-btn \r\n        :commentCount=\"commentCount\" :subscribedCount=\"subscribedCount\" :shareCount=\"shareCount\"/>\r\n      <!-- 播放全部 -->\r\n      <div class=\"play-all\" ref=\"playAll\">\r\n        <img src=\"~assets/img/dailyrecommend/playall/play.svg\" alt=\"\">\r\n        <span>播放全部</span>\r\n      </div>\r\n\r\n      <!-- 音乐列表 -->\r\n      <music-item \r\n        v-for=\"item in songList\" \r\n        :key=\"item.id\"\r\n        :music-item=\"item\"/>\r\n    </scroll>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getSongOrderMsg } from \"network/songOrderMsg\";\r\nimport { Toast } from 'vant';\r\nimport Scroll from 'components/common/scroll/Scroll';\r\nimport MusicItem from 'components/content/musicItem/MusicItem';\r\n\r\nimport Background from './childComps/Background';\r\nimport CollectEtcBtn from './childComps/CollectEtcBtn';\r\n\r\nexport default {\r\n  components: {\r\n    Scroll,\r\n    Background,\r\n    CollectEtcBtn,\r\n    MusicItem\r\n  },\r\n  name: 'SongOrderMsg',\r\n  data() {\r\n    return {\r\n      id: '',   //歌单id\r\n      isFlow: false,//播放全部栏是否吸顶\r\n      songList: [],//歌单歌曲列表\r\n      name: '', //歌单名称\r\n      songOrderImg: '', //歌单图片\r\n      description: '',  //歌单描述\r\n      playCount: 0,     //歌单播放量\r\n      commentCount: 0,  //评论量\r\n      subscribedCount: 0,//收藏量\r\n      shareCount: 0,    //分享量\r\n    };\r\n  },\r\n  methods: {\r\n    /**\r\n     * 事件监听相关方法\r\n     */\r\n    //监听滚动\r\n    contentScroll(position) {\r\n      // console.log(position.y);\r\n      if (Math.abs(position.y) > this.$refs.playAll.offsetTop) {\r\n        this.isFlow = true;\r\n      } else {\r\n        this.isFlow = false;\r\n      }\r\n    },\r\n    /**\r\n     * 网络请求相关方法\r\n     */\r\n    //获取歌单详情\r\n    getSongOrderMsg(id) {\r\n      Toast.loading({\r\n        duration: 0,\r\n        forbidClick: true,\r\n        message: '加载中'\r\n      });\r\n      getSongOrderMsg(id)\r\n        .then(res => {\r\n          console.log('res of song order msg in songordermsg: ', res);\r\n          this.songList = res.playlist.tracks;\r\n          this.name = res.playlist.name;\r\n          this.songOrderImg = res.playlist.coverImgUrl;\r\n          this.description = res.playlist.description;\r\n          this.playCount = res.playlist.playCount;\r\n          this.commentCount = res.playlist.commentCount;\r\n          this.subscribedCount = res.playlist.subscribedCount;\r\n          this.shareCount = res.playlist.shareCount;\r\n        })\r\n        .catch(err => {\r\n          console.log('error of song order msg in songordermsg: ', err);\r\n        })\r\n        .finally(() => {\r\n          Toast.clear();\r\n        })\r\n    }\r\n  },\r\n  created() {\r\n    //获取传过来的id\r\n    this.id = this.$route.query.id;\r\n    this.getSongOrderMsg(this.id);\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.song-order-msg {\r\n  background-color: #f6f6f4;\r\n  overflow: hidden;\r\n  height: calc(100vh - 50px);\r\n}\r\n.play-all {\r\n  display: flex;\r\n  padding: 10px 10px;\r\n  align-items: center;\r\n  font-size: 16px;\r\n  color: #000;\r\n  font-weight: bolder;\r\n  background-color: #f6f6f4;\r\n  margin-top: 40px;\r\n}\r\n.play-all img {\r\n  width: 30px;\r\n  margin-right: 10px;\r\n}\r\n.active {\r\n  z-index: 10;\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  margin-top: 0;\r\n}\r\n.none {\r\n  display: none;\r\n}\r\n</style>"],"sourceRoot":"src/views/songOrderMsg"}]}